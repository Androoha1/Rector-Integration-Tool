{
    "name": "androoha/rector-integration-tool",
    "type": "project",
    "authors": [{ "name": "Andrii Posternak" }],
    "repositories": {
        "configurations": {
            "type": "vcs",
            "url": "git@gitlab.bta.lv:packages/configurations.git"
        }
    },
    "require": {
        "ext-pdo": "*",
        "divi-group/configurations": "^3.1.0",
        "driftingly/rector-laravel": "^2.0.0",
        "rector/rector": "^2.0.0"
    },
    "require-dev": {
        "nikic/php-parser": "^5.4",
        "phpstan/phpstan": "^2.0.0"
    },
    "autoload": {
        "psr-4": {
            "RectorIntegrationTool\\": "src/",
            "RectorIntegrationTool\\database\\": "database/"
        },
        "files": [
            "src/Core/HelperFunctions/coloredText.php"
        ]
    }
}
